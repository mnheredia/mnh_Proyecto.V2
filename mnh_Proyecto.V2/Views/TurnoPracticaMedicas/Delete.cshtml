@model mnh_Proyecto.V2.Models.TurnoPracticaMedica

@{
    ViewData["Title"] = "Delete";
}

<section class="py-0 text-white">
    <div class="container py-4 bg-success bg-gradient">
        <div class="row">
            <div class="col-lg-8 mx-auto text-center">
                <h1>Prácticas Médicas</h1>
                <p class="lead mb-0"><i>Sistema de gestión administrativa</i></p>
            </div>
        </div>
    </div>
</section>
<br />


<h4>Eliminar</h4>

<h5>Está seguro de borrar éste turno de práctica médica?</h5>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdPracticaMedica)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdPracticaMedica)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FechaConsultaMedica)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaConsultaMedica)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DocumentoPaciente)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DocumentoPaciente)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdPaciente)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdPaciente)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DiasDisponibles)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DiasDisponibles)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.HorasDisponibles)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.HorasDisponibles)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver a la lista</a>
    </form>
</div>
